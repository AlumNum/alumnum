<h1>Resume Items</h1>
<% @resume.each do |r| %>
<div class="resume_item" >
<h2><%= r.title %></h2>
<p> <%= r.heading %><br>
<%= r.start_date %> - <%= r.end_date %><br>
<%= r.description %><br>
</p></div>
<% end %>

