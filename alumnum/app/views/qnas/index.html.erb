<p id="notice"><%= notice %></p>

<h1>Listing Qnas</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Question</th>
      <th>Response</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qnas.each do |qna| %>
      <tr>
        <td><%= qna.alum %></td>
        <td><%= qna.question %></td>
        <td><%= qna.response %></td>
        <td><%= link_to 'Show', qna %></td>
        <td><%= link_to 'Edit', edit_qna_path(qna) %></td>
        <td><%= link_to 'Destroy', qna, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qna', new_qna_path %>
